引入 词法分析;
公开 类 启动类
{
	公开 静态 启动()
	{
		词法分析类 c=创建 词法分析类("");
		控制台.输出行("被分析的文本:\t\t");
		c.被分析的文本=控制台.输入文本();
		控制台.输出行("开始分析...\n");
		c.分析();
		int i;
		while(i<c.单词总数)
		{
			控制台.输出(c.单词数组[i]+"\t\t\t\t");
			假如( c.类型数组[i])
			{
			为 单词类型.数值:
				控制台.输出行("数值");
			为 单词类型.标识符:
				控制台.输出行("标识符");
			为 单词类型.字符串:
				控制台.输出行("字符串");
			为 单词类型.运算符:
				控制台.输出行("运算符");
			}
			i++;
		}
		控制台.输出行("\n分析结束.\t\t共"+c.单词总数.到文本+"个单词\n" );
		控制台.输入文本();
	}
}
